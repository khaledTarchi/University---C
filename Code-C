# Display message "Give an integer:"
li $v0, 4
la $a0, message
syscall

# Read an integer
li $v0, 5
syscall

# Save the integer in a register
move $t0, $v0

# Test if the number is positive or negative
bgez $t0, positive
li $v0, 4
la $a0, negative
syscall
j done

positive:
li $v0, 4
la $a0, positive
syscall

done:
